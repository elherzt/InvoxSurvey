# PostgreSQL Local Configuration
POSTGRES_HOST=postgres
POSTGRES_PORT=5432
POSTGRES_DB=YourDbName
POSTGRES_USER=postgres
POSTGRES_PASSWORD=secure_admin_password
ConnectionStrings__Postgres=Host=postgres;Port=5432;Username=postgres;Password=secure_admin_password;Database=YourDbName

# discomment this line to enable database logging
# ENABLE_DB_LOGGING=true
# PostgreSQL Remote Configuration (uncomment to use)
# POSTGRES_SSLMODE=Disable

# Defalt Credentials for autogenerated users
USER_ADMIN_PWD=secure_password
USER_INTERVIEWER_PWS=secure_password
